    BLOCK B14 IN=S21 OUT=S18 GCHOT
    BLOCK B15 IN=S17 OUT=S21
    BLOCK HEATX1 IN=BIOEXIN S8 OUT=BIOEXOUT S2
    BLOCK B11 IN=LAOIN UREAIN S11 OUT=S9
    BLOCK HEATX2 IN=GCHOT S9 OUT=GCPROD S10
    BLOCK B7 IN=METHOUT2 OUT=S3 S7
    BLOCK B17 IN=S4 S20 OUT=S1
